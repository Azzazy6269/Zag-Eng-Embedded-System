#include <stdio.h>
#include <stdlib.h>

int main()
{
    int A,B,C,Min,Max;
    scanf("%d%d%d",&A,&B,&C);
    if(A>B&&A>C){
        Max=A;
    }
    else if(B>A&&B>C){
        Max=B;
    }
    else if(C>A&&C>B){
        Max=C;
    }
    else{
        Max=0;
    }
    if(A<B&&A<C){
        Min=A;
    }
    else if(B<A&&B<C){
        Min=B;
    }
    else if (C<A&&C<B){
        Min=C;
    }
    else{
        Min=0;
    }
    printf("Min=%d\nMax=%d",Min,Max);
    return 0;
}
